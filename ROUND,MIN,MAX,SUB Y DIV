SELECT MIN(AGE) AS 'EDAD MINIMA' -- BUSCAR EL VALOR MINIMO DE LA TABLA 
FROM STUDENTS; 
SELECT MAX(AGE) AS 'EDAD MAXIMA' -- BUSCAR EL VALOR MAXIMO DE LA TABLA 
FROM STUDENTS;
SELECT ROUND(AGE) -- REDONDEAR EL VALOR DE A TABLA
FROM STUDENTS;
SELECT ROUND((MAX(AGE)+MIN(AGE))/2.5) AS RESTA -- SELECCIONAR VALORES Y REDONDEO
FROM STUDENTS; 
