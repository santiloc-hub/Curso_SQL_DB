CREATE TABLE people (--CREAMOS LA PRIMERA TABLA
person_id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, -- CREAMOS LA LLAVE PRIMARIA CON EL ID
last_name VARCHAR(255), -- VARIABLE DE TEXTO CON LONGITUD DE 255
first_name VARCHAR(255), -- VARIABLE DE TEXTO CON LONGITUD DE 255
address VARCHAR(255), -- VARIABLE DE TEXTO CON LONGITUD DE 255
city VARCHAR(255) -- VARIABLE DE TEXTO CON LONGITUD DE 255
);
