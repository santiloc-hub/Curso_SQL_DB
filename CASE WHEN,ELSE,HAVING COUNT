SELECT STUDENTID,COUNT(*) -- MOSTRAR CUANTOS ESTUDIANTES HAY CON EL MISMO ID
FROM STUDENT_COURSES
GROUP BY STUDENTID;
SELECT COURSESID,COUNT(*) -- MOSTRAR CUANTOS CUANTOS CURSOS HAY CON EL MISMO ID
FROM STUDENT_COURSES
GROUP BY COURSESID
HAVING COUNT(*)>1;
SELECT * FROM STUDENTS;

SELECT FIRSTNAME,AGE, CASE WHEN FIRSTNAME='Emily' AND AGE BETWEEN 18 AND 24 THEN 'GROUP A' ELSE 'GROUP B'END -- AGRUPAR LOS DATOS Y APLICAR SENTENCIAS DE WHEN
FROM STUDENTS
GROUP BY FIRSTNAME,AGE;

