SELECT * FROM INSTRUCTORS,ESTUDIANTE
WHERE ESTUDIANTE.AGE<50 OR INSTRUCTORS.AGE<30;-- SELECCIONAR DOS TABLAS Y REALIZAR LA BUSQUEDA
