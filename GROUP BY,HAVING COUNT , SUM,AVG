SELECT COURSESID,COUNT(STUDENTID) -- CONTAR CUANTOS ESTUDIANTES HAY EN UN CURSO
FROM student_courses
GROUP BY COURSESID -- AGRUPAR EN CURSOS
HAVING COUNT(STUDENTID)>1;-- COMPARAR CON LOS MAYORES A 1
SELECT SUM(AGE) AS 'SUMA DE EDADES' FROM STUDENTS; -- SUMAR TODA UNA COLUMNA
SELECT AVG(AGE) AS 'PROMEDIO DE EDADES' FROM STUDENTS; --PROMEDIO DE UNA COLUMNA

SELECT * FROM STUDENT_COURSES
